<% if notice %>
  <div id="validation_msgs">
  	<br>
	<div class="alert alert-info" role="alert" style="margin: 0px">
	<%= notice %>
	</div>
  </div>
<% elsif alert %>
  <div id="validation_msgs">
  	<br>
	<div class="alert alert-danger" role="alert" style="margin: 0px">
	   <%= alert %>
	</div>
  </div>
<% end %>